{
    "name": "Eye Dropper",
    "description": "It picks colors",
    "version": "0.0.1",
    "manifest_version": 3,
    "action": {
        "default_popup": "./popup.html"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "./contentScript.js"
            ]
        }
    ],
    "permissions": [
        "tabs",
        "storage"
    ],
    "host_permissions": [
        "<all_urls>"
    ]
}